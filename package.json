{
  "name": "surf-weather",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node ./dist/src/index.js",
    "start:dev": "ts-node-dev ./src/index.ts",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "style:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "test:unit-watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michelboller/surf-weather.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Michel Boller Ticiani <michelboller1@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michelboller/surf-weather/issues"
  },
  "homepage": "https://github.com/michelboller/surf-weather#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "0.0.41",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.22",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^7.2.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^0.26.1",
    "body-parser": "^1.19.2",
    "config": "^3.3.7",
    "express": "^4.17.3",
    "module-alias": "^2.2.2"
  }
}
